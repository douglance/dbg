{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://dbg.dev/schemas/AttachRequest.schema.json",
	"title": "AttachRequest",
	"type": "object",
	"additionalProperties": false,
	"required": ["provider", "bundleId"],
	"properties": {
		"provider": {
			"type": "string",
			"enum": ["apple-device"]
		},
		"platform": {
			"type": "string",
			"enum": ["auto", "ios", "tvos", "watchos", "visionos"]
		},
		"bundleId": {
			"type": "string",
			"minLength": 1
		},
		"device": {
			"type": "string",
			"minLength": 1
		},
		"pid": {
			"type": "integer",
			"minimum": 1
		},
		"launch": {
			"type": "boolean"
		},
		"protocol": {
			"type": "string",
			"enum": ["dap", "cdp"]
		},
		"attachStrategy": {
			"type": "string",
			"enum": ["auto", "device-process", "gdb-remote"]
		},
		"attachTimeoutMs": {
			"type": "integer",
			"minimum": 1
		},
		"verbose": {
			"type": "boolean"
		}
	}
}
